{
  "name": "frontend-temp",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:unit:run": "vitest run",
    "test:unit:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:mobile": "playwright test mobile.spec.ts",
    "test:regression": "playwright test --project=regression",
    "test:regression:headed": "playwright test --project=regression --headed",
    "test:regression:mobile": "playwright test --project=regression-mobile",
    "test:regression:auth": "playwright test tests/e2e/regression/auth/",
    "test:regression:dashboard": "playwright test tests/e2e/regression/dashboard/",
    "test:regression:members": "playwright test tests/e2e/regression/members/",
    "test:regression:navigation": "playwright test tests/e2e/regression/navigation/",
    "test:regression:notes": "playwright test tests/e2e/regression/notes/",
    "test:regression:badges": "playwright test tests/e2e/regression/badges/",
    "test:regression:attributes": "playwright test tests/e2e/regression/attributes/",
    "test:regression:families": "playwright test tests/e2e/regression/families/",
    "test:regression:all": "playwright test --project=regression --project=regression-mobile",
    "test:regression:report": "playwright show-report test-results/regression-report",
    "test:all": "npm run test:unit:run && npm run test:e2e",
    "test:ci": "npm run test:unit:coverage && npm run test:e2e && npm run test:regression",
    "test:full": "npm run test:unit:coverage && npm run test:e2e && npm run test:regression:all",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@quasar/extras": "^1.17.0",
    "@quasar/vite-plugin": "^1.10.0",
    "@sentry/vite-plugin": "^4.3.0",
    "@sentry/vue": "^10.18.0",
    "@vueuse/core": "^13.9.0",
    "axios": "^1.12.2",
    "idb": "^8.0.3",
    "pinia": "^3.0.3",
    "qrcode": "^1.5.4",
    "quasar": "^2.18.5",
    "vis-network": "^10.0.2",
    "vite-plugin-pwa": "^1.0.3",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1",
    "workbox-window": "^7.3.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@pinia/testing": "^1.0.2",
    "@playwright/test": "^1.55.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.18.6",
    "@types/qrcode": "^1.5.6",
    "@types/workbox-window": "^4.3.4",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "@vitest/eslint-plugin": "^1.4.2",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.10.3",
    "eslint": "^9.33.0",
    "eslint-plugin-playwright": "^2.2.2",
    "eslint-plugin-vue": "~10.4.0",
    "jiti": "^2.5.1",
    "jsdom": "^27.0.0",
    "npm-run-all2": "^8.0.4",
    "postcss": "^8.5.6",
    "prettier": "3.6.2",
    "sass-embedded": "^1.93.2",
    "tailwindcss": "^3.4.18",
    "typescript": "~5.9.0",
    "vite": "^7.1.7",
    "vite-plugin-vue-devtools": "^8.0.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.0"
  }
}
